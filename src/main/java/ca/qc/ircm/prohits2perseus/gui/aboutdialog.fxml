<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.labjacquespe.vap.gui.*?>
<?scenebuilder-preview-i18n-resource aboutdialog.properties?>

<BorderPane styleClass="about-dialog" xmlns:fx="http://javafx.com/fxml" fx:controller="ca.qc.ircm.prohits2perseus.gui.AboutDialogPresenter">
	<center>
		<HBox styleClass="messages">
			<Label fx:id="message" text="%message" minHeight="50"/>
		</HBox>
	</center>
	<bottom>
		<HBox styleClass="buttons">
			<Button fx:id="ok" text="%ok" onAction="#close" defaultButton="true" cancelButton="true"/>
		</HBox>
	</bottom>
</BorderPane>
